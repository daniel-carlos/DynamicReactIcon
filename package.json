{
  "name": "dynamic-react-icon",
  "version": "1.0.0",
  "description": "A dynamic React component for rendering react-icons dynamically based on icon name",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --clean",
